[
  {
    "defaults": {
      "name": "global",
      "project-type": "freestyle",
      "concurrent": true,
      "description": "Do not edit this job through the web!",
      "disabled": false,
      "quiet-period": 5,
      "block-downstream": false,
      "block-upstream": false,
      "retry-count": 3,
      "triggers": [
        {
          "pollscm": {
            "cron": "* * * * *"
          }
        }
      ],
      "scm": [
        {
          "git": {
            "url": "https://github.com/PDAxh/Jenkins",
            "clean": {
              "after": true
            },
            "repo-depth": "infinity",
            "wipe-workspace": true,
            "git-config-name": "confname",
            "git-config-email": "confemail"
          }
        }
      ],
      "notifications": [
        {
          "http": {
            "url": "http://10.2.2.33:3300/notification",
            "format": "json",
            "event": "finalized",
            "timeout": 30000,
            "log": 0
          }
        }
      ]
    }
  },
  {
    "job": {
      "name": "CalculatorProject-maven-job",
      "node": "master",
      "builders": [
        {
          "maven-target": {
            "maven-version": "Maven3",
            "pom": "pom.xml",
            "goals": "clean package findbugs:findbugs checkstyle:checkstyle"
          }
        },
        {
          "maven-builder": {
            "name": "mvn3",
            "pom": "pom.xml",
            "goals": "clean project"
          }
        }
      ],
      "properties": [
        {
          "github": {
            "url": "https://github.com/PDAxh/Jenkins"
          }
        }
      ],
      "triggers": [
        "github",
        {
          "pollscm": {
            "cron": "* * * * *"
          }
        }
      ]
    }
  }
]