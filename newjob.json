[
  {
    "defaults": {
      "name": "global",
      "project-type": "freestyle",
      "concurrent": true,
      "description": "Do not edit this job through the web!",
      "disabled": false,
      "quiet-period": 5,
      "block-downstream": false,
      "block-upstream": false,
      "retry-count": 3,
      "triggers": [
        {
          "pollscm": {
            "cron": "* * * * *"
          }
        }
      ],
      "scm": [
        {
          "git": {
            "url": "ger.giter",
            "clean": {
              "after": true
            },
            "repo-depth": "infinity",
            "wipe-workspace": true,
            "git-config-name": "confname",
            "git-config-email": "confemail"
          }
        }
      ],
      "notifications": [
        {
          "http": {
            "url": "http://10.90.131.114:3300/notification",
            "format": "json",
            "event": "finalized",
            "timeout": 30000,
            "log": 0
          }
        }
      ]
    }
  },
  {
    "job": {
      "name": "danieltest6-maven-job",
      "node": "master",
      "builders": [
        {
          "maven-target": {
            "maven-version": "Maven3",
            "pom": "pom.xml",
            "goals": "clean package findbugs:findbugs checkstyle:checkstyle"
          }
        },
        {
          "maven-builder": {
            "name": "mvn3",
            "pom": "pom.xml",
            "goals": "clean project"
          }
        }
      ],
      "properties": [
        {
          "github": {
            "url": "ger.giter"
          }
        }
      ],
      "triggers": [
        "github",
        {
          "pollscm": {
            "cron": "* * * * *"
          }
        }
      ],
      "publishers": [
        {
          "checkstyle": {
            "pattern": "**/checkstyle-result.xml",
            "healthy": 0,
            "unhealthy": 100,
            "health-threshold": "high",
            "thresholds": {
              "unstable": {
                "total-high": 10
              },
              "failed": {
                "total-high": 1
              }
            }
          }
        }
      ]
    }
  }
]